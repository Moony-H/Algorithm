fun main() {
    val diff= 'a'.code -'A'.code
    val word=readln()
    val hash=HashMap<Int,Int>()
    word.forEach {c->
        val temp=c.code
        val cInt=if(temp>='a'.code) temp-diff else temp
        hash[cInt]?.let {
            hash[cInt]=it+1
        }?:run{
            hash[cInt]=1
        }
    }
    val sortedHash=hash.toSortedMap()
    val a=sortedHash.pollLastEntry()
    

}